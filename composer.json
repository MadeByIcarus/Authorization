  "keywords": [
    "nette",
    "latte",
    "mail",
    "mail templates",
    "doctrine2"
  ],
  "authors": [
    {
      "name": "Pavel GajdoÅ¡",
      "email": "info@pavelgajdos.cz"
    },
    {
      "name": "Icarus s.r.o",
      "homepage": "http://icarus.cz"
    }
  ],
  "require": {
    "php": ">=7.3",
    "nette/security": "^3.0",
    "nette/application": "^3.0"
  },
  "require-dev": {
    "nette/tester": "^2.2"
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "autoload": {
    "psr-4": {
      "Icarus\\Authorization\\": "src/"
